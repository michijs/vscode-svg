{
  "name": "@michijs/vscode-svg",
  "displayName": "SVG 2",
  "description": "SVG extension for VS Code",
  "author": "Lucas M. Segurado",
  "icon": "media/svg.png",
  "version": "2.1.4",
  "publisher": "michijs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michijs/vscode-svg.git"
  },
  "exports": {
    "./svg.json": "./generated/svg.json",
    "./attributeSets.json": "./generated/attributeSets.json"
  },
  "files": [
    "generated/"
  ],
  "galleryBanner": {
    "color": "#5663da",
    "theme": "dark"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/michijs"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.38.0"
  },
  "scripts": {
    "generate": "bun run ./tasks/generateTypes.ts",
    "test-tsc": "tsc",
    "package": "change-package-name vscode-svg && vsce package && change-package-name @michijs/vscode-svg",
    "publish-vsce": "vsce publish"
  },
  "keywords": [
    "math",
    "svg",
    "html",
    "typescript"
  ],
  "devDependencies": {
    "@mdn/browser-compat-data": "7.3.2",
    "@michijs/shared-configs": "0.0.36",
    "@types/bun": "1.3.9",
    "@vscode/vsce": "3.7.1",
    "@vscode/web-custom-data": "0.6.2",
    "change-package-name": "1.0.5",
    "compute-baseline": "0.4.0",
    "typescript": "5.9.3",
    "vscode-html-languageservice": "5.6.1"
  },
  "contributes": {
    "html": {
      "customData": [
        "./generated/svg.json"
      ]
    }
  }
}